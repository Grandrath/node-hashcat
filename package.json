{
  "author": "Mendhak",
  "name": "hashcat",
  "description": "Commandline application to concatenate, minify, cache-bust and replace JS references",
  "version": "0.2.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/mendhak/node-hashcat.git"
  },
  "main": "hashcat",
